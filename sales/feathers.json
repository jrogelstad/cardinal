                "description": "Promise date",
                "type": "string",
                "format": "date",
                "isRequired": true
            },
            "shipped": {
                "description": "Quantity shipped",
                "type": "number",
                "isReadOnly": true
            },
            "shipInstructions": {
                "description": "Shipment Instructions",
