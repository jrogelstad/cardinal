[ 
    {
        "name": "SalesOrder",
        "plural": "SalesOrders",
        "module": "Sales",
        "inherits": "ShipOrder",
        "description": "Sales order",
        "properties": {
            "number": {
                "description": "Sales order number",
                "isReadOnly": true,
                "type": "string",
                "autonumber": {
                    "prefix": "S",
                    "sequence": "salesorder_seq_num",
                    "length": 5
                },
                "isUnique": true,
                "inheritedFrom": "BillOrder"
            },
            "requestDate": {
                "description": "Requsted date to ship",
                "type": "string",
                "format": "date",
                "default": "today()"
            },
            "promiseDate": {
                "description": "Promise date to ship",
                "type": "string",
                "format": "date",
                "default": "today()"
            }
        },
        "overloads": {
            "billEntity": {
                "alias": "Customer",
                "type": {
                    "relation": "Customer",
                    "properties": ["code", "description", "terms",
                        "billTo", "shipTo", "isActive"] 
                },
                "isRequired": true
            },
            "status": {
                "description": "Order status",
                "type": "string",
                "default": "O",
                "isReadOnly": true,
                "isRequired": true,
                "format": "enum",
                "dataList": [
                    {
                        "value": "O",
                        "label": "Open"
                    },
                    {
                        "value": "S",
                        "label": "Shipping"
                    },
                    {
                        "value": "P",
                        "label": "Partial"
                    },
                    {
                        "value": "C",
                        "label": "Closed"
                    }
                ]
            }
        }
    },
    {
