                "isRequired": true
            }
        }
    },
    {
        "name": "Voucher",
        "plural": "Vouchers",
        "module": "Payables",
        "inherits": "Payable",
        "description": "Documentation for vendor invoices that increase accounts payable",
        "properties": {
            "number": {
                "description": "Voucher number",
                "isReadOnly": true,
                "type": "string",
                "autonumber": {
                    "prefix": "VCH",
                    "sequence": "voucher_seq_num",
                    "length": 5
                },
                "isUnique": true,
                "inheritedFrom": "BillOrder"
            },
            "invoiceNumber": {
                "description": "Vendor's invoice number",
                "type": "string"
            }
        },
        "overloads": {
            "billEntity": {
                "alias": "Vendor",
                "type": {
                    "relation": "Vendor",
                    "properties": ["code", "description", "billTo",
                        "currency", "terms", "taxType", "isActive"] 
                },
                "isRequired": true
            },
            "docDate": {
                "alias": "invoiceDate"
            }
        }
    },
    {
        "name": "VoucherLine",
        "module": "Payables",
        "description": "Voucher line",
        "inherits": "PayableLine",
        "properties": {
            "parent": {
              "description": "Parent voucher",
              "type": {
                    "relation": "Voucher",
                    "childOf": "lines"
                }
            }
        }
    },
    {
        "name": "DebitMemo",
        "plural": "DebitMemos",
        "module": "Paybles",
        "inherits": "Payable",
        "description": "Reduces accounts payable typically a refund for a return",
        "properties": {
            "number": {
                "description": "Debit memo number",
                "isReadOnly": true,
                "type": "string",
                "autonumber": {
                    "prefix": "DM",
                    "sequence": "debit_memo_seq_num",
                    "length": 5
                },
                "isUnique": true,
                "inheritedFrom": "BillOrder"
            }
        },
        "overloads": {
            "billEntity": {
                "alias": "vendor",
                "type": {
                    "relation": "Vendor",
                    "properties": ["code", "description", "billTo",
                        "currency", "terms", "taxType", "isActive"] 
                },
                "isRequired": true
            },
            "docDate": {
                "alias": "memoDate"
            }
        }
    },
    {
        "name": "DebitMemoLine",
        "module": "Payables",
        "description": "Payable line",
        "inherits": "PayableLine",
        "properties": {
            "parent": {
              "description": "Parent debit memo",
              "type": {
                    "relation": "DebitMemo",
                    "childOf": "lines"
                }
            }
        },
        "overloads": {
            "billed": {
                "alias": "credited"
            }
        }
    },
    {
        "name": "PayablesJournal",
        "plural": "PayablesJournals",
        "module": "Payables",
        "description": "Accounts payable journal",
