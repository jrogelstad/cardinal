        "properties": {
            "number": {
                "description": "Journal number",
                "isReadOnly": true,
                "type": "string",
                "autonumber": {
                    "prefix": "AP",
                    "sequence": "ap_seq_num",
                    "length": 5
                },
                "isUnique": true,
                "inheritedFrom": "Journal"
            }
        }
    }
]
