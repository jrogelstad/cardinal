[
  {
    "name": "Pay",
    "module": "Payables",
    "launchConfig": {
      "icon": "sign-out"
    },
    "defaultConfig": [{
        "name": "Vouchers",
        "isEditModeEnabled": false,
        "feather": "Voucher",
        "actions": [{
          "name": "Post",
          "title": "Post selected unposted vouchers",
          "icon": "thumbtack",
          "method": "post",
          "authorization": "PostPayables",
          "validator": "postCheck"
        }, {
          "name": "Post all",
          "title": "Post all unposted vouchers",
          "icon": "thumbtack",
          "method": "postAll",
          "authorization": "PostPayables",
          "hasSeparator": true
        }],
        "list": {
            "columns": [{
              "attr": "number"
            }, {
              "attr": "vendor"
            }, {
              "attr": "vendor.description"
            }, {
              "attr": "total"
            }, {
              "attr": "docDate"
            }, {
              "attr": "dueDate"
            }, {
              "attr": "postedDate"
            }, {
              "attr": "status"
            }],
            "filter": {
                "criteria": [{
                    "property": "status",
                    "operator": "!=",
                    "value": "C"
                }],
                "sort": [{
                    "property": "dueDate",
                    "order": "ASC"
                }, {
                    "property": "number",
                    "order": "ASC"
                }]
            }
        }
    },
    {
        "name": "Debit Memos",
        "isEditModeEnabled": false,
        "feather": "DebitMemo",
        "actions": [{
          "name": "Post",
          "title": "Post selected unposted debit memos",
          "icon": "thumbtack",
          "method": "post",
          "authorization": "PostPayables",
          "validator": "postCheck"
        }, {
          "name": "Post all",
          "title": "Post all unposted debit memos",
          "icon": "thumbtack",
          "method": "postAll",
          "authorization": "PostPayables",
          "hasSeparator": true
        }],
        "list": {
            "columns": [{
              "attr": "number"
            }, {
              "attr": "vendor"
            }, {
              "attr": "vendor.description"
            }, {
              "attr": "total"
            }, {
              "attr": "docDate"
            }, {
              "attr": "dueDate"
            }, {
              "attr": "postedDate"
            }, {
              "attr": "status"
            }],
            "filter": {
                "criteria": [{
                    "property": "status",
                    "operator": "!=",
                    "value": "C"
                }],
                "sort": [{
                    "property": "dueDate",
                    "order": "ASC"
                }, {
                    "property": "number",
                    "order": "ASC"
                }]
            }
        }
    },
    {
        "name": "Journals",
        "feather": "PayablesJournal",
        "actions": [{
          "name": "Post",
          "title": "Post selected unposted journals to the ledger",
          "icon": "thumbtack",
          "method": "post",
          "authorization": "PostJournals",
          "validator": "postCheck"
        }, {
          "name": "Post all",
          "title": "Post all unposted journals to the ledger",
          "icon": "thumbtack",
          "method": "postAll",
          "authorization": "PostJournals",
          "hasSeparator": true
        }],
        "list": {
          "columns": [{
              "attr": "number"
            }, {
              "attr": "date"
            }, {
              "attr": "amount"
            }, {
              "attr": "isPosted"
            }
          ],
          "filter": {
            "sort": [{
                "property": "isPosted",
                "order": "DESC"
            }, {
                "property": "number",
                "order": "DESC"
            }]
          }
        }
      },
    {
        "name": "Vendors",
        "feather": "Vendor",
        "list": {
          "columns": [{
              "attr": "code"
            }, {
              "attr": "description"
            }, {
              "attr": "terms.code",
              "label": "Terms"
            }, {
              "attr": "contact.fullName"
            }, {
              "attr": "contact.phone"
            }, {
              "attr": "contact.email"
            }, {
              "attr": "remitTo.city"
            }, {
              "attr": "remitTo.state"
            }
          ],
          "filter": {
            "criteria": [{
                "property": "isActive",
                "value": true
            }],
            "sort": [{
                "property": "code"
            }]
          }
        }
      }
    ]
  }
]
