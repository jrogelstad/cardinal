[ 
    {
        "name": "Customer",
        "plural": "Customers",
        "module": "Receivables",
        "description": "Customer",
        "inherits": "Kind",
        "properties": {
            "code": {
                "description": "Customer code",
                "isReadOnly": true,
                "type": "string",
                "autonumber": {
                    "prefix": "C",
                    "sequence": "customer_seq_num",
                    "length": 5
                },
                "isUnique": true,
                "inheritedFrom": "Node"
            },
            "terms": {
                "description": "Payment terms",
                "type": {
                    "relation": "Terms",
                    "properties": ["code", "description", "policy",
                        "net", "day", "discountDays", "discount",
                        "isDepositRequired", "depositPercent", "depositAmount"]
                }
            },
            "billTo": {
                "description": "Bill to Address",
                "type": {
                    "relation": "Address",
                    "properties": ["type", "street", "unit", "city",
                        "state", "postalCode", "country"]
                }
            },
            "shipTo": {
                "description": "Ship to Address",
                "type": {
                    "relation": "Address",
                    "properties": ["type", "street", "unit", "city",
                        "state", "postalCode", "country"]
                }
            },
            "contact": {
                "description": "Contact",
                "type": {
                  "relation": "Contact",
                  "properties": ["fullName", "email", "phone", "address"]
                }
            },
            "creditStatus": {
                "description": "creditStatus",
                "type": "string",
                "format": "enum",
                "isRequired": true,
                "default": "G",
				"dataList": [{
                    "value": "G",
                    "label": "Good"
                }, {
                    "value": "W",
                    "label": "Warn"
                }, {
                    "value": "H",
                    "label": "Hold"
                }]
            },
            "isActive": {
                "description": "Active status",
                "type": "boolean",
                "default": true
            }
        },
        "overloads": {
            "description": {
                "alias": "name"
            }
        }
    },
    {
        "name": "AccountsReceivable",
        "plural": "AccountsReceivable",
        "module": "Receivables",
        "description": "Accounts receivable balance",
        "inherits": "Node"
    },
    {
        "name": "AccountsReceivableJournal",
        "plural": "AccountsReceivableJournal",
        "module": "Receivables",
        "description": "Accounts receivable journal",
        "inherits": "Journal",
        "properties": {
            "number": {
                "description": "Journal number",
                "isReadOnly": true,
                "type": "string",
                "autonumber": {
                    "prefix": "AR",
                    "sequence": "ar_seq_num",
                    "length": 5
                },
                "isUnique": true,
                "inheritedFrom": "Journal"
            }
        }
    }
]
