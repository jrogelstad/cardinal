[
    {
        "name": "CustomerCategory",
        "plural": "CustomerCategories",
        "module": "Common",
        "description": "Customer category",
        "inherits": "Category",
        "overloads": {
            "parent": {
                "type": {
                    "relation": "CustomerCategory",
                    "properties": ["code", "description"]
                }
            }
        }
    },
    {
        "name": "Customer",
        "plural": "Customers",
        "module": "Receivables",
        "description": "Customer",
        "inherits": "BillEntity",
        "properties": {
            "creditStatus": {
                "description": "creditStatus",
                "type": "string",
                "format": "enum",
                "isRequired": true,
                "default": "G",
				"dataList": [{
                    "value": "G",
                    "label": "Good"
                }, {
                    "value": "W",
                    "label": "Warn"
                }, {
                    "value": "H",
                    "label": "Hold"
                }]
            }
        },
        "overloads": {
            "description": {
                "alias": "name"
            },
            "category": {
                "description": "Customer category",
                "type": {
                    "relation": "CustomerCategory"
                }
            }
        }
    },
    {
        "name": "Receivable",
        "plural": "Receivables",
        "module": "Receivables",
        "description": "Receivable subledger",
        "inherits": "BillSubledger"
    },
    {
        "name": "ReceivableLine",
        "module": "Receivables",
        "description": "Receivable line",
        "inherits": "OrderLine",
        "properties": {
            "billed": {
                "description": "Quantity billed",
                "type": "number",
                "isRequired": true
            }
        }
    },
    {
        "name": "Invoice",
        "plural": "Invoices",
        "module": "Receivables",
        "inherits": "Receivable",
        "description": "Invoice",
        "properties": {
            "number": {
                "description": "Invoice number",
                "isReadOnly": true,
                "type": "string",
                "autonumber": {
                    "prefix": "INV",
                    "sequence": "invoice_seq_num",
                    "length": 5
                },
                "isUnique": true,
                "inheritedFrom": "BillOrder"
            }
        },
        "overloads": {
            "billEntity": {
                "alias": "Customer",
                "type": {
                    "relation": "Customer",
                    "properties": ["code", "description", "billTo",
                        "currency", "terms", "taxType", "isActive"] 
                },
                "isRequired": true
            },
            "docDate": {
                "alias": "invoiceDate"
            }
        }
    },
    {
        "name": "InvoiceLine",
        "module": "Receivables",
        "description": "Invoice line",
        "inherits": "ReceivableLine",
        "properties": {
            "parent": {
              "description": "Parent invoice",
              "type": {
                    "relation": "Invoice",
                    "childOf": "lines"
                }
            }
        }
    },
    {
        "name": "CreditMemo",
        "plural": "CreditMemos",
        "module": "Receivables",
        "inherits": "Receivable",
        "description": "Credit memo",
        "properties": {
            "number": {
                "description": "Credit memo number",
                "isReadOnly": true,
                "type": "string",
                "autonumber": {
                    "prefix": "CM",
                    "sequence": "credit_memo_seq_num",
                    "length": 5
                },
                "isUnique": true,
                "inheritedFrom": "BillOrder"
            }
        },
        "overloads": {
            "billEntity": {
                "alias": "customer",
                "type": {
                    "relation": "Customer",
                    "properties": ["code", "description", "billTo",
                        "currency", "terms", "taxType", "isActive"] 
                },
                "isRequired": true
            },
            "docDate": {
                "alias": "memoDate"
            }
        }
    },
    {
        "name": "CreditMemoLine",
        "module": "Receivables",
        "description": "Receivable line",
        "inherits": "ReceivableLine",
        "properties": {
            "parent": {
              "description": "Parent credit memo",
              "type": {
                    "relation": "CreditMemo",
                    "childOf": "lines"
                }
            }
        },
        "overloads": {
            "billed": {
                "alias": "credited"
            }
        }
    },
    {
        "name": "ReceivablesJournal",
        "plural": "ReceivablesJournals",
        "module": "Receivables",
        "description": "Accounts receivable journal",
        "inherits": "Journal",
        "properties": {
            "number": {
                "description": "Journal number",
                "isReadOnly": true,
                "type": "string",
                "autonumber": {
                    "prefix": "AR",
                    "sequence": "ar_seq_num",
                    "length": 5
                },
                "isUnique": true,
                "inheritedFrom": "Journal"
            }
        }
    }
]
